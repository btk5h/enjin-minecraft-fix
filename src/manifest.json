{
  "manifest_version": 2,
  "name": "Enjin Minecraft Avatar Fix",
  "description": "Displays 1.8 skin layers on Enjin websites",
  "version": "1.0.1",
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "https://api.mojang.com/",
    "storage",
    "unlimitedStorage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["patch.js"],
      "run_at": "document_end"
    }
  ]
}